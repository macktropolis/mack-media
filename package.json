  "license": "ISC",
  "bugs": {
    "url": "https://github.com/macktropolis/jammin-to-eleventy/issues"
  },
  "homepage": "https://github.com/macktropolis/jammin-to-eleventy#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-plugin-rss": "^1.1.1",
    "concurrently": "^6.2.0",
    "gulp": "^4.0.2",
    "nunjucks": "^3.2.3",
    "sass": "^1.35.1"
  }
}
