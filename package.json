{
  "name": "jammin-to-eleventy",
  "version": "1.0.0",
  "description": "A Jamstack site framework using Eleventy and SASS.",
  "main": ".eleventy.js",
  "scripts": {
    "watch:sass": "npx sass src/sass:public/css --watch",
    "watch:eleventy": "npx @11ty/eleventy --serve",
    "start": "concurrently \"npm run watch:eleventy\" \"npm run watch:sass\"",
    "build": "eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/macktropolis/jammin-to-eleventy.git"
  },
